{
  "compilerOptions": {
    "target": "ES2017",
    "module": "ESNext",
    "moduleResolution": "Node",
		"lib": [
			"ES2017",
			"DOM"
		],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "types": [
      "@types/node",
      "types-mediawiki"
    ],
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "global.d.ts"
  ]
}